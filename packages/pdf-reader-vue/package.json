{
  "name": "@otools/pdf-reader-vue",
  "version": "0.0.1",
  "description": "pdf reader for vue",
  "keywords": [
    "vue",
    "pdf"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "lint": "eslint ./src --ext=.vue",
    "lint:fix": "eslint ./src --ext=.vue --fix",
    "build": "npm run build:library",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/index.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/index.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/index.min.js",
    "build:library": "rimraf ./dist & npm run build:unpkg & npm run build:es & npm run build:umd & npm run build:unpkg",
    "test": "jest"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.7",
    "pdfjs-dist": "^2.0.943"
  },
  "devDependencies": {
    "@otools/compile-tools": "0.0.4",
    "rollup": "^2.42.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "vue": ">=2.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/owenvip/outils.git"
  },
  "author": "owenwong <owen.wong.cn@gmail.com> (https://owenvip.github.io/)",
  "license": "MIT",
  "prettier": "@otools/prettier-config-otool"
}
