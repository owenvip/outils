    "es",
    "lib",
    "index.d.ts"
  ],
  "prettier": "@otools/prettier-config-otool",
  "devDependencies": {
    "@otools/compile-tools": "0.0.12"
  },
  "dependencies": {
    "babel-plugin-import": "^1.13.3"
  }
}
