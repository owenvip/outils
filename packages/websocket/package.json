{
  "name": "@otools/websocket",
  "version": "0.0.0",
  "description": "websocket util for browser",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src --ext=.ts",
    "lint:fix": "eslint ./src --ext=.ts --fix",
